{
  "name": "dev-quest-journey-to-fullstack",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "dev:back": "nodemon server",
    "dev:front": "vite",
    "prestart": "npx sequelize db:create",
    "mergefork": "git remote add upstream https://github.com/StephaneRavet/Devs-Quest-Journey-to-Full-Stack.git && git fetch upstream && git checkout main && git merge upstream/main"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "vite": "^4.1.0"
  }
}
